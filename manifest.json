{
  "name": "Transmitter",
  "description": "Send magnet torrent links to Transmission",
  "version": "0.2",
  "permissions": [
    "contextMenus",
    "notifications",
    "http://*/*"
  ],
  "background": {
    "scripts": ["jquery.js", "transmitter.js"]
  },
  "icons" : {
    "16": "icons/Transmission_16.png",
    "32": "icons/Transmission_32.png",
    "64": "icons/Transmission_64.png",
    "128": "icons/Transmission_128.png",
    "256": "icons/Transmission_256.png",
    "512": "icons/Transmission_512.png"
  },
  "web_accessible_resources" : [
    "icons/Transmission_32.png",
    "icons/Transmission_64.png",
    "icons/Transmission_128.png"
  ],
  "options_page": "options.html",
  "manifest_version": 2
}
